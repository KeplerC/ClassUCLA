        subjects = subjects.replace(/&quot;/g, '"');
        GlobalFunctions.CreateAutocomplete('select_filter_subject', $.parseJSON(subjects.replace(/&amp;/g, '&')), 'Enter Subject Area (Required)', '_Select_Subject');        
    });
</script>
</div>
