<input aria-label="Search" autocomplete="off" class="NormalTextBox" id="dnn_jscsHeader_dnnSEARCH_txtSearch" maxlength="255" name="dnn$jscsHeader$dnnSEARCH$txtSearch" placeholder="Search this site..." size="20" type="text"/>
